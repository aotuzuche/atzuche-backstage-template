    "core-js": "^3.8.0",
    "dva-core": "1.4.0",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-use": "^15.3.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/dva": "^1.1.0",
    "@types/mockjs": "^1.0.3",
    "@types/react": "^16.9.49",
