    "react-dom": "^16.13.1",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-use": "^14.2.0"
  },
  "devDependencies": {
    "autos": "^1.1.6",
    "babel-eslint": "^10.0.1",
    "babel-preset-atzuche": "^0.1.0",
    "eslint": "^6.7.2",
