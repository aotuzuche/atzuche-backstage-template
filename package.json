    "@types/react-router-dom": "^5.1.5",
    "@types/webpack-env": "^1.15.2",
    "autos": "^4.2.10",
    "babel-eslint": "^10.0.1",
    "babel-preset-atzuche": "^0.5.0",
    "eslint": "^7.9.0",
    "eslint-config-atzuche": "^3.0.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "minimist": "^1.2.0",
    "prettier": "^2.2.1",
    "qs": "^6.9.4",
    "typescript": "^4.0.3",
    "yamljs": "^0.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
