    "babel-preset-atzuche": "^0.2.0",
    "eslint": "^7.3.1",
    "eslint-config-atzuche": "^1.0.2",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
